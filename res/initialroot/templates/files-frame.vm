<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!-- NewPage -->
<html lang="en">
<head>
<meta charset="utf-8"/>
<!-- Generated by statdoc (version $version) on $date -->
<title>statdoc</title>
<meta name="date" content="$date_short">
<link rel="stylesheet" type="text/css" href="../stylesheet.css" title="Style">
<script src="../d3.min.js"></script>
<script src="../statdoc.js"></script>
</head>
<body>
#macro( fileList $t $items )
<div class="indexContainer">
<h2 title="$t"><a name="$t"><!-- --></a><a href="../files/files-summary.html#$t"  target="classFrame" >$t</a></h2>
<ul title="$t">
#foreach( $item in $items )
#if( $item.getType().startsWith("group:") )
##<li><a href="../files/files-summary.html${item.getLink()}%20${item.getFullName()}" title="File group $item.getFullName()"  target="classFrame" >+ $item.getAbbrevName(25) [$item.getChildren().size()]</a></li>
#set( $uniqueId = "file_${t}_${item.hashCode()}")
<li><a onclick='toggle("$uniqueId");' title="File group $item.getFullName()"  target="classFrame" >+ $item.getAbbrevName(25) [$item.getChildren().size()]</a></li>
<div id="$uniqueId" style="display: none;">
#foreach( $gitem in $item.getChildren() )
<li style="background: ${gitem.myColor()};">&nbsp;&nbsp;<a href="../${gitem.getLink()}" title="$gitem.getFullName()"  target="classFrame" >$gitem.getAbbrevName(30)</a></li>
#end
</div>
#else
<li style="background: ${item.myColor()};"><a href="../${item.getLink()}" title="$item.getFullName()"  target="classFrame" >$item.getAbbrevName(30)</a></li>
#end
#end
</ul>
</div>
#end
<h1 class="bar"><a href="../files/files-summary.html"  target="classFrame" >Files summary</a></h1>
#fileList( "Data" $filesItem.getGroupedChildrenBy("file:data") )
#fileList( "Scripts" $filesItem.getGroupedChildrenBy("file:script") )
#fileList( "Images" $filesItem.getGroupedChildrenBy("file:image") )
#fileList( "Documents" $filesItem.getGroupedChildrenBy("file:doc") )
#fileList( "Other" $filesItem.getGroupedChildrenBy("file:general") )
</div>
</body>
</html>
