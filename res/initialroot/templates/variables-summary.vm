#if ( $est.size() == 0 )
#if ( "$color" == "altColor" ) #set( $color = "rowColor" ) #else #set( $color = "altColor" ) #end
<tr class="$color">
<td class="colFirst"><a href="../${var.getLink()}" title="variable $var">$var</a></td>
<td class="colLast">
#foreach( $subvar in $var.getChildrenBy( "variable:" ) )
